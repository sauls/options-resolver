{
  "name": "sauls/options-resolver",
  "description": "Symfony options resolver with multi dimensional array support",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Saulius Vaičeliūnas",
      "email": "saulius@vaiceliunas.lt"
    }
  ],
  "require": {
    "php": ">=7.4",
    "sauls/collections": "^1.0",
    "symfony/options-resolver": "^4.0 || ^5.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "php-coveralls/php-coveralls": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "Sauls\\Component\\OptionsResolver\\": "src/"
    },
    "exclude-from-classmap": [
      "../tests"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Sauls\\Component\\OptionsResolver\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "extra": {
    "branch-alias": {
      "dev-master": "v1.x-dev"
    }
  }
}
